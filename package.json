{
  "name": "@cowtech/react-lazily",
  "version": "2.5.0",
  "description": "CowTech React components and utilities.",
  "homepage": "https://sw.cowtech.it/react-lazily",
  "repository": "github:cowtech/react-lazily",
  "keywords": [
    "react",
    "lazily"
  ],
  "bugs": {
    "url": "https://github.com/cowtech/react-lazily/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "files": [
    "images",
    "dist",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "main": "dist/cjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.mjs"
    },
    "./images/cowtech.png": "./images/cowtech.png",
    "./images/favicon-32.png": "./images/favicon-32.png",
    "./images/favicon-192.png": "./images/favicon-192.png",
    "./images/favicon-196.png": "./images/favicon-196.png"
  },
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/*.ts src/**/*.ts src/**/*.tsx",
    "prebuild": "rm -rf dist types && yarn lint",
    "build": "tsc -p . && tsc -p tsconfig.modules.json && renamer --find js --replace mjs dist/mjs/* dist/mjs/**/* >> /dev/null && jscodeshift -s --extensions=mjs -t node_modules/@cowtech/esm-package-utils dist/mjs/** && echo '' >> types/index.d.ts && cat src/utils/sitemap.d.ts >> types/index.d.ts",
    "postpublish": "git push origin && git push origin -f --tags",
    "update:normalize": "node scripts/update-normalize.js"
  },
  "peerDependencies": {
    "idb-keyval": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typestyle": "^2.1.0"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^7.14.0",
    "@cowtech/esm-package-utils": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "got": "^11.8.1",
    "idb-keyval": "^3.2.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "typescript": "^4.1.3",
    "typestyle": "^2.1.0"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
