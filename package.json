{
  "name": "@cowtech/react-lazily",
  "version": "9.1.0",
  "description": "CowTech React components and utilities.",
  "homepage": "https://sw.cowtech.it/react-lazily",
  "repository": "github:cowtech/react-lazily",
  "keywords": [
    "react",
    "lazily"
  ],
  "bugs": {
    "url": "https://github.com/cowtech/react-lazily/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "files": [
    "images",
    "dist",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "swc --delete-dir-on-start -w -d dist src",
    "build": "swc --delete-dir-on-start -d dist src",
    "postbuild": "concurrently npm:lint npm:typecheck && cat src/utils/sitemap.d.ts.tpl >> dist/index.d.ts && cp -a src/assets dist",
    "format": "prettier -w src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "typecheck": "tsc -p . --emitDeclarationOnly",
    "test": "exit 0",
    "ci": "npm run build",
    "prepublishOnly": "npm run ci",
    "postpublish": "git push origin && git push origin -f --tags",
    "update:normalize": "curl -sSL https://cdn.jsdelivr.net/npm/modern-normalize@1.1.0/modern-normalize.min.css | prettier --parser css | ghead -n -1 > src/assets/styles/normalize.css"
  },
  "peerDependencies": {
    "@unocss/core": "^0.58.2",
    "@unocss/preset-mini": "^0.58.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^9.0.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@unocss/core": "^0.58.2",
    "@unocss/preset-mini": "^0.58.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">= 18.18.0"
  }
}
