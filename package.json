{
  "name": "@cowtech/react-lazily",
  "version": "3.1.4",
  "description": "CowTech React components and utilities.",
  "homepage": "https://sw.cowtech.it/react-lazily",
  "repository": "github:cowtech/react-lazily",
  "keywords": [
    "react",
    "lazily"
  ],
  "bugs": {
    "url": "https://github.com/cowtech/react-lazily/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "files": [
    "images",
    "dist",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./images/cowtech.png": "./images/cowtech.png",
    "./images/favicon-32.png": "./images/favicon-32.png",
    "./images/favicon-192.png": "./images/favicon-192.png",
    "./images/favicon-196.png": "./images/favicon-196.png",
    "./images/cowtech.webp": "./images/cowtech.webp",
    "./images/favicon-32.webp": "./images/favicon-32.webp",
    "./images/favicon-192.webp": "./images/favicon-192.webp",
    "./images/favicon-196.webp": "./images/favicon-196.webp"
  },
  "typings": "./types/index.d.ts",
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "swc -s -w -d dist src",
    "prebuild": "rm -rf dist types && npm run typecheck && npm run lint",
    "build": "swc --delete-dir-on-start -d dist src",
    "postbuild": "echo '' >> types/index.d.ts && cat src/utils/sitemap.d.ts.tpl >> types/index.d.ts",
    "format": "prettier -w src",
    "typecheck": "tsc -p . --emitDeclarationOnly",
    "lint": "eslint src",
    "ci": "npm run build && npm run lint",
    "prepublishOnly": "npm run ci",
    "postpublish": "git push origin && git push origin -f --tags",
    "update:normalize": "node --loader ts-node/esm scripts/update-normalize.ts"
  },
  "peerDependencies": {
    "fela": "^12.0.2",
    "idb-keyval": "^6.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fela": "^12.0.2"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^8.7.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.244",
    "@types/node": "^18.7.13",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "chokidar": "^3.5.3",
    "fela": "^12.2.0",
    "idb-keyval": "^6.2.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-fela": "^12.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "undici": "^5.10.0"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
